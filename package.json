{
  "name": "nextjs-cloudflare-start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "check-seo": "node scripts/validate-seo.mjs",
    "build": "pnpm check-seo && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "init-project": "node scripts/init-project.mjs",
    "indexnow": "node scripts/submit-indexnow.mjs",
